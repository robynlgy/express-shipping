{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:3000/shipping.schema.json",
    "title": "Shipping",
    "description": "Shipment for product.",
    "type": "object",
    "properties": {
      "productId": {
          "type": "integer",
          "minimum": 1000
        },
      "name": {"type": "string"},
      "addr": {"type": "string"},
      "zip": {"type": "string"}
    },
    "additionalProperties": false,
    "required": [
      "productId",
      "name",
      "addr",
      "zip"
    ]
}
